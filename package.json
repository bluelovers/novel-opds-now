    "calibre-opds": "^1.0.5",
    "cjk-conv": "^1.2.40",
    "cross-fetch": "^3.0.4",
    "cross-spawn-extra": "^2.0.0",
    "debug-color2": "^1.1.7",
    "dot-values2": "^2.0.0",
    "epub-maker2": "^2.4.23",
    "express": "^4.17.1",
    "file-type": "^14.1.2",
    "formidable": "^1.2.1",
    "fs-extra": "^8.1.0",
    "gun": "^0.2020.116",
    "gun-tag": "^3.0.2",
