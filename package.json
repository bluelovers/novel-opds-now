    "test": "echo \"Error: no test specified\" && exit 1",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "npm:publish": "npm publish",
    "lerna:publish:yes": "yarn run prepublishOnly && yarn run prepublish:commit && npx lerna publish --yes --cd-version patch",
    "prepublish:cache": "ts-node ./cli/cache.ts -f",
    "prepublish:commit": "git commit -m \"chore: update cache\" .",
    "prepublish:update-deps": "yarn run ncu && yarn run sort-package-json && yarn run update-deps",
    "prepublishOnly": "yarn run prepublish:update-deps && yarn run prepublish:cache",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json",
    "update-deps": "npx yarn-tool install"
  },
